<!doctype html>
<head>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width,initial-scale=1">
<meta name="robots" content="noindex, nofollow">
<title>vim-jp &raquo; vim-jp.slack.com log - &#35{{ .channel.Name }} - {{ .monthKey.Year }}年{{ .monthKey.Month }}月</title>
<link rel="stylesheet" href="{{ $.baseURL }}/assets/css/site.css" type="text/css" />
<link rel="stylesheet" href="https://unpkg.com/@primer/css/dist/primer.css" type="text/css" />
<link rel="stylesheet" href="https://unpkg.com/prismjs@1.20.0/themes/prism-tomorrow.css" type="text/css" />
<link rel="alternate" type="application/rss+xml" title="RSS" href="//vim-jp.org/rss.xml" />
<link rel="canonical" href="{{ $.baseURL }}/{{ .channel.ID }}/{{ .monthKey.Year }}/{{ .monthKey.Month }}/" />
<link rel="shortcut icon" type="image/x-icon" href="/assets/images/favicon.ico" />
<link rel="icon" type="image/x-icon" href="/assets/images/favicon.ico" />
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
<script src="https://unpkg.com/prismjs@1.20.0/components/prism-core.min.js"></script>
<script src="https://unpkg.com/prismjs@1.20.0/plugins/autoloader/prism-autoloader.min.js"></script>
<script src="{{ $.baseURL }}/assets/javascripts/slacklog.js"></script>
</head>
<body>
  <div class="body">
    <div id="content">
      <!-- header -->
      <div class="pagehead ml-3">
        <h1>
          <span class="author">
            <a href="//vim-jp.org" class="url fn" >vim-jp</a>
          </span>
          <span class="path-divider">/</span>
          <a href="{{ $.baseURL }}">slacklog</a>
        </h1>
      </div>
      <!-- /header -->
      <div>
        <div class="m-3">
          <nav aria-label="Breadcrumb">
            <ol>
              <li class="breadcrumb-item f4"><a href="{{ $.baseURL }}/{{ .channel.ID }}/">&#35{{ .channel.Name }}</a></li>
              <li class="breadcrumb-item f4" aria-current="page">{{ .monthKey.Year }}年{{ .monthKey.Month }}月</li>
            </ol>
          </nav>
          <h4 class="text-gray pb-2 border-bottom"></h4>
        </div>
        {{- range .msgs }}
        {{- if visible . }}
        <div class="clearfix m-3" id="ts-{{ .Ts }}">
          <div class="border-bottom">
            <div class="float-left mr-2">
              <img class="avatar" width="36" height="36" src="{{ userIconUrl . }}" />
            </div>
            <div>
              <span class="text-bold mr-1">{{ username . }}</span>
              <a href="#ts-{{ .Ts }}">{{ datetime .Ts }}</a>
              <span class="Label Label--outline">
                <a href="https://vim-jp.slack.com/archives/{{ $.channel.ID }}/p{{ slackPermalink .Ts }}">Slack</a>
              </span>
            </div>
            <div class="overflow-hidden mb-3">
              <div class="pt-1 pb-1">
                {{ text . }}
              </div>

              <div>
                {{- if reactions . }}
                  {{- range reactions . }}
                    <span class="tooltipped tooltipped-ne tooltipped-align-left-1 m-2 p-2 border" aria-label="{{ .Users }}">
                      {{- if eq .Default false }}
                        <img class="slacklog-emoji" alt="{{ .Name }}" title="{{ .Name }}" src="{{ .EmojiLink }}">{{ .Count }}
                      {{- else if eq .Default true }}
                        <span>{{ .Name }}{{ .Count }}</span>
                      {{- end }}
                    </span>
                  {{- end }}
                {{- end }}
              </div>

            {{- if and (ne .ThreadTs "") (ne .ThreadTs .Ts) }}
            <span class="f6 text-gray-light">
              このスレッドに返信しました : <a href="#ts-{{- .ThreadTs }}">{{- threadRootText .ThreadTs }}</a>
            </span>
            {{- end }}

              {{- if .Attachments }}
              <div class="mt-2 border p-2" id="ts-{{ .Ts }}">
              {{- range .Attachments }}
                {{- if eq .ServiceName "GitHub" }}
                  <div class="d-flex">
                    <div>
                      <img class="avatar avatar-small" width="32" height="32" src="{{ .ServiceIcon }}" alt="{{ html .ServiceName }}" />
                    </div>
                    <div class="flex-self-baseline f4 ml-2">
                      <span class="Label Label--outline">
                        <a href="{{ .TitleLink }}">{{ html .Title }}</a>
                      </span>
                    </div>
                  </div>
                  <div class="d-flex pl-2 mb-2">
                    <div class="mt-2">{{ attachmentText . }}</div>
                  </div>
                {{- else if eq .ServiceName "twitter" }}
                  <div class="d-flex">
                    <div>
                      <img class="avatar avatar-small" width="32" height="32" src="{{ .AuthorIcon }}" alt="{{ html .ServiceName }}" />
                    </div>
                    <div class="flex-self-baseline f4 ml-2">
                      <span class="f6 text-gray-light">{{ .AuthorName }} {{ .AuthorSubname }}</span>
                    </div>
                  </div>
                  <div class="d-flex">
                    <div class="mt-2">{{ attachmentText . }}</div>
                  </div>
                  <div class="d-flex">
                    <div>
                      <img class="avatar avatar-small" width="20" height="20" src="{{ .FooterIcon }}" />
                    </div>
                    <div class="flex-self-baseline f5 ml-2">
                      {{ html .Footer }}
                    </div>
                  </div>
                  {{- if .VideoHTML }}
                  <div class="d-flex mt-1">
                    {{ .VideoHTML }}
                  </div>
                  {{- end }}
                {{- else if eq .ServiceName "Gyazo" }}
                    <div class="d-flex">
                      <div>
                        <!-- Gyazo -->
                        <a href="{{ .Fallback }}">{{ .Fallback }}</a>
                      </div>
                    </div>
                {{- else if or .Title .Text }}
                  <div class="d-flex">
                    {{- if and .Title .TitleLink }}
                      {{- if and .ServiceIcon .ServiceName }}
                      <div>
                        <img class="avatar avatar-small" width="32" height="32" src="{{ .ServiceIcon }}" alt="{{ html .ServiceName }}" />
                      </div>
                      {{- end }}
                      <div class="flex-self-baseline f4 ml-2">
                        <span class="Label Label--outline">
                          <a href="{{ .TitleLink }}">{{ html .Title }}</a>
                        </span>
                      </div>
                    {{- else if .Title }}
                      <div>{{ html .Title }}</div>
                    {{- end }}
                  </div>
                  {{- if .Text }}
                    <div class="d-flex">
                      <div class="mt-2">{{ attachmentText . }}</div>
                    </div>
                  {{- end }}

                  {{- if .ThumbURL }}
                    <div class="d-flex">
                      <div class="mt-2">
                        <img src="{{ .ThumbURL }}" width="{{ .ThumbWidth }}" height="{{ .ThumbHeight }}" alt="{{ html .Title }}" />
                      </div>
                    </div>
                  {{- end }}
                {{- end }}
              {{- end }}
            </div>
            {{- end }}

            {{- if .Files }}
              <div class="mt-2 border p-3">
                {{- range .Files }}
                <div>
                  {{- if .IsSlackHosted }}
                  <a href="{{ $.baseURL }}/files/{{ .OriginalFilePath }}">
                  {{- if eq .TopLevelMimetype "image" }}
                  <img src="{{ $.baseURL }}/files/{{ .ThumbImagePath }}" width="{{ .ThumbImageWidth }}" height="{{ .ThumbImageHeight }}" alt="{{ .Title }}" />
                  {{- else if eq .TopLevelMimetype "video" }}
                  <video src="{{ $.baseURL }}/files/{{ .OriginalFilePath }}" poster="{{ $.baseURL }}/files/{{ .ThumbVideoPath }}" controls alt="{{ .Title }}">
                  </video>
                  {{- else }}
                  <span class="f5">[[ダウンロード: {{ .Title }}({{ .PrettyType }})]]</span>
                  {{- end }}
                  </a>
                  {{- if eq .Mimetype "text/plain" }}
                  <pre>{{ fileHTML . }}</pre>
                  {{- end }}
                  {{- else }}
                  {{- /* TODO: 外部ページへのリンクだということがわかりやすい表示にする */ -}}
                  <a href="{{ .URLPrivate }}">{{ .Title }}</a>
                  {{- end }}
                </div>
                {{- end }}
              </div>
            {{- end }}

            {{- if threads .Ts }}
              <details class="details-reset mt-3">
                <summary class="btn-link">
                  <span class="f5">
                  {{- threadNum .ThreadTs }} 件の返信 最終返信:{{- threadMtime .ThreadTs }} <span class="dropdown-caret"></span>
                  </span>
                </summary>
                <div class="border mt-2">
                  {{- range threads .Ts }}
                  <div class="p-2" id="ts-{{ .Ts }}">
                    {{- if eq .Subtype "thread_broadcast" }}
                    <div class="d-flex flex-wrap">
                      <div class="f6 ml-3 text-gray-light">#←</div>
                      <div class="f6 ml-2 text-gray-light">チャンネルにも投稿済</div>
                    </div>
                    {{- end }}
                    <div class="float-left mr-2">
                      <img class="avatar" width="36" height="36" src="{{ userIconUrl . }}" />
                    </div>
                    <div>
                      <span class="text-bold mr-1">{{ username . }}</span>
                      <a href="#ts-{{ .Ts }}">{{ datetime .Ts }}</a>
                    </div>
                    <div class="overflow-hidden">
                      {{ text . }}
                    </div>

                    {{- if .Attachments }}
                      {{- range .Attachments }}
                      {{- if eq .ServiceName "GitHub" }}
                        <div class="d-flex">
                          <div>
                            <img class="avatar avatar-small" width="32" height="32" src="{{ .ServiceIcon }}" alt="{{ html .ServiceName }}" />
                          </div>
                          <div class="flex-self-baseline f4 ml-2">
                            <span class="Label Label--outline">
                              <a href="{{ .TitleLink }}">{{ html .Title }}</a>
                            </span>
                          </div>
                        </div>
                        <div class="d-flex pl-2 mb-2">
                          <div class="mt-2">{{ attachmentText . }}</div>
                        </div>
                      {{- else if eq .ServiceName "twitter" }}
                        <div class="d-flex">
                          <div>
                            <img class="avatar avatar-small" width="32" height="32" src="{{ .AuthorIcon }}" alt="{{ html .ServiceName }}" />
                          </div>
                          <div class="flex-self-baseline f4 ml-2">
                            <span class="f6 text-gray-light">{{ .AuthorName }} {{ .AuthorSubname }}</span>
                          </div>
                        </div>
                        <div class="d-flex">
                          <div class="mt-2">{{ attachmentText . }}</div>
                        </div>
                        <div class="d-flex">
                          <div>
                            <img class="avatar avatar-small" width="20" height="20" src="{{ .FooterIcon }}" />
                          </div>
                          <div class="flex-self-baseline f5 ml-2">
                            {{ html .Footer }}
                          </div>
                        </div>
                        {{- if .VideoHTML }}
                        <div class="d-flex mt-1">
                          {{ .VideoHTML }}
                        </div>
                        {{- end }}
                      {{- else if eq .ServiceName "Gyazo" }}
                        <div class="d-flex">
                          <div>
                            <!-- Gyazo -->
                            <a href="{{ .Fallback }}">{{ .Fallback }}</a>
                          </div>
                        </div>
                      {{- else if or .Title .Text }}
                        <div class="d-flex">
                          {{- if and .Title .TitleLink }}
                            {{- if and .ServiceIcon .ServiceName }}
                            <div>
                              <img class="avatar avatar-small" width="32" height="32" src="{{ .ServiceIcon }}" alt="{{ html .ServiceName }}" />
                            </div>
                            {{- end }}
                            <div class="flex-self-baseline f4 ml-2">
                              <span class="Label Label--outline">
                                <a href="{{ .TitleLink }}">{{ html .Title }}</a>
                              </span>
                            </div>
                          {{- else if .Title }}
                            <div>{{ html .Title }}</div>
                          {{- end }}
                        </div>
                        {{- if .Text }}
                          <div class="d-flex">
                            <div class="mt-2">{{ attachmentText . }}</div>
                          </div>
                        {{- end }}

                        {{- if .ThumbURL }}
                          <div class="d-flex">
                            <div class="mt-2">
                              <img src="{{ .ThumbURL }}" width="{{ .ThumbWidth }}" height="{{ .ThumbHeight }}" alt="{{ html .Title }}" />
                            </div>
                          </div>
                        {{- end }}
                      {{- end }}
                      {{- end }}
                    {{- end }}
                  </div>
                  {{- end }}
                </div>
              </details>
            {{- end }}
            </div>
          </div>
        </div>
        {{- end }}
        {{- end }}
      </div>
    </div>
    <!-- footer -->
    <div class="clearfix"></div>
    <div id="footer">
      <p>
        Powered by <a href="https://github.com/">GitHub</a>
      </p>
    </div>
    <!-- /footer -->
  </div>
</body>
